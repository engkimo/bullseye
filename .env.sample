# AWS Configuration
AWS_PROFILE=your-profile
AWS_REGION=ap-northeast-1
KEY_NAME=your-ec2-key
S3_BUCKET=docja-checkpoints-your-account-id

# Hugging Face
HF_TOKEN=your-huggingface-token
HF_REPO=your-username/docja-weights
HF_PUBLAYNET_REPO=
HF_DOCBANK_REPO=

# Bullseye model repos on Hugging Face (defaults)
DOCJA_BULLSEYE_DET_REPO=Ryousukee/bullseye-dbnet
DOCJA_BULLSEYE_REC_REPO=Ryousukee/bullseye-recparseq
DOCJA_BULLSEYE_LAYOUT_REPO=Ryousukee/bullseye-layoutrtdetrv
DOCJA_BULLSEYE_TABLE_REPO=Ryousukee/bullseye-tablertdetrv

# Dataset Paths (optional - can be set via registry)
DATASET_SYNTHDOG_JA_PATH=/path/to/synthdog-ja
DATASET_DOCLAYNET_PATH=/path/to/doclaynet
DATASET_PUBTABNET_PATH=/path/to/pubtabnet
DATASET_JSQUAD_PATH=/path/to/jsquad
DATASET_JAQUAD_PATH=/path/to/jaquad

# Model Paths
MODEL_BASE_PATH=gpt-oss-20B
WEIGHTS_DIR=weights

# Training Configuration
CUDA_VISIBLE_DEVICES=0
PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:256
TOKENIZERS_PARALLELISM=false

# vLLM Configuration
VLLM_PORT=8000
VLLM_GPU_MEMORY_UTILIZATION=0.9

# Logging
LOG_LEVEL=INFO
WANDB_PROJECT=docja
WANDB_API_KEY=your-wandb-key

# API
# Require API Key for REST endpoints. Set a strong random string.
DOCJA_API_KEY=

# Dataset downloader (scripts/download_datasets.sh)
# Provide URLs when prompted or predefine env vars below (no direct links stored in repo)
# SYNTHDOG_URL=
# DOCLAYNET_URL=
# PUBTABNET_URL=

# Development
DEBUG=false
TESTING=false
